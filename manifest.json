{
  "manifest_version": 3,
  "name": "Tecmicra - Add New Task",
  "version": "1.0",
  "description": "Open Tecmicra dashboard to add new task",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage"]
}
